  <title>Configurations of the Aldoses</title>
  <metadata xmlns:md="http://cnx.rice.edu/mdml">
    <md:title>Configurations of the Aldoses</md:title>
    <md:content-id>m00302</md:content-id>
    <md:uuid>be9d1de8-9066-4f22-9035-9a1b70628e19</md:uuid>
