<document xmlns="http://cnx.rice.edu/cnxml">
  <title>Mass Spectrometry of Some Common Functional Groups</title>
  <metadata xmlns:md="http://cnx.rice.edu/mdml">
    <md:title>Mass Spectrometry of Some Common Functional Groups</md:title>
    <md:content-id>m00137</md:content-id>
    <md:uuid>b6d3ae2a-16e4-46a2-963c-1d4fcc775baa</md:uuid>
