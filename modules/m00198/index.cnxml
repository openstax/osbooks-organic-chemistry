      <item><link document="m00188" target-id="term-00001">electrophilic aromatic substitution</link></item>
      <item><link document="m00191" target-id="term-00002">Friedel–Crafts reaction</link></item>
      <item><link document="m00192" target-id="term-00001">inductive effect</link></item>
      <item><link document="m00194" target-id="term-00001">nucleophilic aromatic substitution</link></item>
